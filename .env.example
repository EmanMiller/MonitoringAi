# Copy to .env and fill in real values. Never commit .env.

# Dan watcher (npm run watch:dan, npm run dev:all)
GITHUB_USERNAME=your_github_username
GITHUB_EMAIL=your_email@example.com
GITHUB_TOKEN=ghp_your_personal_access_token

# Optional: backend integrations (DashboardApi loads from env)
# GEMINI_API_KEY=EMAN_GOOGLE_API_KEY_HERE
# Sumo Logic (for Create Dashboard wizard - get from Sumo Logic Personal Access Keys)
# SUMO_LOGIC_ACCESS_ID=your_access_id
# SUMO_LOGIC_ACCESS_KEY=your_access_key
# SUMO_LOGIC_FOLDER_ID=your_folder_id  # From Sumo Logic: Manage Data > folder URL or API
# SUMO_LOGIC_API_URL=https://api.sumologic.com  # Or api.us2.sumologic.com, api.eu.sumologic.com for your region
